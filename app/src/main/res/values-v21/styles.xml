        <item name="android:textAllCaps">false</item>
        <item name="strokeColor">@color/colorPrimaryDark</item>
    </style>
    <style name="FacebookButton">
        <item name="android:backgroundTint">@color/bleu_fb</item>
    </style>
</resources>